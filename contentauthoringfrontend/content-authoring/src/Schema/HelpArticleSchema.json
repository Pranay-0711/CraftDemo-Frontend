{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "subtitle": {
        "type": "string"
      },
      "image": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          },
          "altText": {
            "type": "string"
          }
        },
        "required": ["url", "altText"]
      },
      "paragraph": {
        "type": "string"
      }
    },
    "required": ["title", "subtitle", "image", "paragraph"]
  }